{
  "name": "email-voucher-cms",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "start:local": "sls offline --stage offline --httpPort 4602 --reloadHandler",
    "docs": "mkdir -p docs && serverless openapi generate -o docs/openapi.json -f json -p docs/postman.json",
    "docs:sync": "yarn docs && aws s3 cp ./docs/openapi.json s3://docs.evcms.tport.dev/openapi.json --acl public-read",
    "invoke-rce": "serverless invoke local -f registerChannelEmail -p ./src/functions/registerChannelEmail/mock.json",
    "invoke-aouc": "serverless invoke local -f addOrUpdateChannelAuth -p ./src/functions/addOrUpdateChannelAuth/mock.json",
    "invoke-aoup": "serverless invoke local -f addOrUpdateParsers -p ./src/functions/addOrUpdateParsers/mock.json",
    "invoke-amg": "serverless invoke local -f addMotherEmailGroup -p ./src/functions/addMotherEmailGroup/mock.json",
    "invoke-rpe": "serverless invoke local -f registerPropertyEmail -p ./src/functions/registerPropertyEmail/mock.json",
    "invoke-aourpm": "serverless invoke local -f addOrUpdateRatePlanMapping -p ./src/functions/addOrUpdateRatePlanMapping/mock.json",
    "invoke-pen": "serverless invoke local -f processEmailNotification -p ./src/functions/processEmailNotification/mock.json",
    "invoke-grpe": "serverless invoke local -f getRegisteredPropertyEmail -p ./src/functions/getRegisteredPropertyEmail/mock.json",
    "invoke-gprpm": "serverless invoke local -f getPropertyRatePlanMappings -p ./src/functions/getPropertyRatePlanMappings/mock.json"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.485.0",
    "@middy/core": "^3.4.0",
    "@middy/http-cors": "^4.2.8",
    "@middy/http-json-body-parser": "^3.4.0",
    "@openapi-contrib/openapi-schema-to-json-schema": "^4.0.4",
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/html-to-text": "^9.0.0",
    "ajv": "^8.12.0",
    "axios": "^1.3.4",
    "chrono-node": "^2.6.3",
    "email-addresses": "^5.0.0",
    "google-auth-library": "^8.7.0",
    "googleapis": "^113.0.0",
    "html-to-text": "^9.0.5",
    "json-schema-to-ts": "^1.5.0",
    "mongodb": "^5.1.0",
    "mongoose": "^7.0.3",
    "node-fetch": "^3.3.1",
    "passive-promise": "^0.4.0",
    "redis": "^4.6.5",
    "serverless-dotenv-plugin": "^4.0.2",
    "serverless-offline-ssm": "^6.2.0"
  },
  "devDependencies": {
    "@tportio/eslint-config": "^0.2.5",
    "@tportio/tsconfig": "^0.1.1",
    "@types/jest": "^29.4.4",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "esbuild": "^0.14.11",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "serverless": "^3.38.0",
    "serverless-esbuild": "^1.23.3",
    "serverless-offline": "^12.0.4",
    "serverless-openapi-documenter": "^0.0.48",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
